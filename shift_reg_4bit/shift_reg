S_0x55eeb3a7caf0 .scope module, "shift_test" "shift_test" 2 1;
 .timescale 0 0;
v0x55eeb3a90f10_0 .var "clk", 0 0;
v0x55eeb3a90fd0_0 .var "clr", 0 0;
v0x55eeb3a910a0_0 .var "in", 0 0;
v0x55eeb3a911a0_0 .net "out", 0 0, v0x55eeb3a90dd0_0;  1 drivers
S_0x55eeb3a7cc80 .scope module, "SR" "shift_reg_4bit" 2 6, 3 51 0, S_0x55eeb3a7caf0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "clock";
    .port_info 2 /INPUT 1 "clear";
    .port_info 3 /OUTPUT 1 "e";
v0x55eeb3a43ba0_0 .net "a", 0 0, v0x55eeb3a910a0_0;  1 drivers
v0x55eeb3a909b0_0 .var "b", 0 0;
v0x55eeb3a90a70_0 .var "c", 0 0;
v0x55eeb3a90b40_0 .net "clear", 0 0, v0x55eeb3a90fd0_0;  1 drivers
v0x55eeb3a90c00_0 .net "clock", 0 0, v0x55eeb3a90f10_0;  1 drivers
v0x55eeb3a90d10_0 .var "d", 0 0;
v0x55eeb3a90dd0_0 .var "e", 0 0;
E_0x55eeb3a425b0/0 .event negedge, v0x55eeb3a90b40_0;
E_0x55eeb3a425b0/1 .event posedge, v0x55eeb3a90c00_0;
E_0x55eeb3a425b0 .event/or E_0x55eeb3a425b0/0, E_0x55eeb3a425b0/1;
    .scope S_0x55eeb3a7cc80;
T_0 ;
    %wait E_0x55eeb3a425b0;
    %load/vec4 v0x55eeb3a90b40_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a909b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a90a70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a90d10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a90dd0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55eeb3a90d10_0;
    %assign/vec4 v0x55eeb3a90dd0_0, 0;
    %load/vec4 v0x55eeb3a90a70_0;
    %assign/vec4 v0x55eeb3a90d10_0, 0;
    %load/vec4 v0x55eeb3a909b0_0;
    %assign/vec4 v0x55eeb3a90a70_0, 0;
    %load/vec4 v0x55eeb3a43ba0_0;
    %assign/vec4 v0x55eeb3a909b0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55eeb3a7caf0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a90f10_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a90fd0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55eeb3a90fd0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55eeb3a7caf0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55eeb3a90f10_0;
    %inv;
    %store/vec4 v0x55eeb3a90f10_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55eeb3a7caf0;
T_3 ;
    %delay 2, 0;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a910a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eeb3a910a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55eeb3a910a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55eeb3a910a0_0, 0, 1;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %end;
    .thread T_3;
    .scope S_0x55eeb3a7caf0;
T_4 ;
    %vpi_call 2 29 "$dumpfile", "shift.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55eeb3a7caf0 {0 0 0};
    %delay 200, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
