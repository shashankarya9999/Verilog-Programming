#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x564b57e44590 .scope module, "cyclic_lamp_tb" "cyclic_lamp_tb" 2 1;
 .timescale 0 0;
v0x564b57e56a30_0 .var "clk", 0 0;
v0x564b57e56b00_0 .net "light", 0 2, v0x564b57e56800_0;  1 drivers
S_0x564b57e44720 .scope module, "LAMP" "cyclic_lamp" 2 5, 3 1 0, S_0x564b57e44590;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /OUTPUT 3 "light";
P_0x564b57e08cf0 .param/l "GREEN" 0 3 6, C4<010>;
P_0x564b57e08d30 .param/l "RED" 0 3 6, C4<100>;
P_0x564b57e08d70 .param/l "YELLOW" 0 3 6, C4<001>;
P_0x564b57e08db0 .param/l "s0" 0 3 5, +C4<00000000000000000000000000000000>;
P_0x564b57e08df0 .param/l "s1" 0 3 5, +C4<00000000000000000000000000000001>;
P_0x564b57e08e30 .param/l "s2" 0 3 5, +C4<00000000000000000000000000000010>;
v0x564b57e41f60_0 .net "clock", 0 0, v0x564b57e56a30_0;  1 drivers
v0x564b57e56800_0 .var "light", 0 2;
v0x564b57e568e0_0 .var "state", 0 1;
E_0x564b57e423b0 .event posedge, v0x564b57e41f60_0;
    .scope S_0x564b57e44720;
T_0 ;
    %wait E_0x564b57e423b0;
    %load/vec4 v0x564b57e568e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x564b57e56800_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x564b57e568e0_0, 0;
    %jmp T_0.4;
T_0.0 ;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x564b57e56800_0, 0;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x564b57e568e0_0, 0;
    %jmp T_0.4;
T_0.1 ;
    %pushi/vec4 1, 0, 3;
    %assign/vec4 v0x564b57e56800_0, 0;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x564b57e568e0_0, 0;
    %jmp T_0.4;
T_0.2 ;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x564b57e56800_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x564b57e568e0_0, 0;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564b57e44590;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x564b57e56a30_0;
    %inv;
    %store/vec4 v0x564b57e56a30_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x564b57e44590;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564b57e56a30_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x564b57e44590;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "cyclic.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564b57e44590 {0 0 0};
    %vpi_call 2 16 "$monitor", "t=%0d, RGY: %b", $time, v0x564b57e56b00_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
