:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55e456120360 .scope module, "counter_7bit_tb" "counter_7bit_tb" 2 1;
 .timescale 0 0;
v0x55e456132910_0 .var "clk", 0 0;
v0x55e4561329e0_0 .var "clr", 0 0;
v0x55e456132ab0_0 .net "out", 7 0, v0x55e4561327a0_0;  1 drivers
S_0x55e4561204f0 .scope module, "DUT" "counter" 2 6, 3 1 0, S_0x55e456120360;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clear";
    .port_info 1 /INPUT 1 "clock";
    .port_info 2 /OUTPUT 8 "count";
P_0x55e4561206d0 .param/l "N" 0 3 3, +C4<00000000000000000000000000000111>;
v0x55e4560e8ba0_0 .net "clear", 0 0, v0x55e4561329e0_0;  1 drivers
v0x55e4561326e0_0 .net "clock", 0 0, v0x55e456132910_0;  1 drivers
v0x55e4561327a0_0 .var "count", 0 7;
E_0x55e4560e7760 .event negedge, v0x55e4561326e0_0;
    .scope S_0x55e4561204f0;
T_0 ;
    %wait E_0x55e4560e7760;
    %load/vec4 v0x55e4560e8ba0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55e4561327a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e4561327a0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x55e4561327a0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e456120360;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e456132910_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55e456120360;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55e456132910_0;
    %inv;
    %store/vec4 v0x55e456132910_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e456120360;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e4561329e0_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e4561329e0_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e4561329e0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55e456120360;
T_4 ;
    %vpi_call 2 22 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e456120360 {0 0 0};
    %vpi_call 2 24 "$monitor", $time, " Count: %d", v0x55e456132ab0_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
